{
  "name": "aks-music",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "preinstall": "npx only-allow yarn",
    "lint": "eslint . --max-warnings=0",
    "format": "prettier . --write",
    "lint:fix": "eslint . --fix",
    "prepare": "husky install"
  },
  "dependencies": {
    "expo": "~50.0.14",
    "expo-status-bar": "~1.11.1",
    "react": "18.2.0",
    "react-native": "0.73.6"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0",
    "@eslint/js": "^9.0.0",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/react": "~18.2.45",
    "eslint": "^9.0.0",
    "eslint-config-universe": "^12.0.0",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "typescript": "^5.1.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ],
    "*.{js,jsx,ts,tsx,json,md,cjs,mjs}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=20",
    "yarn": ">=4.1.1"
  },
  "packageManager": "yarn@4.1.1"
}
